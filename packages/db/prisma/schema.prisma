generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users{
  id                String      @id @default(uuid())
  email             String      @unique
  password          String
  username          String
}
model Story{
  id                Int         @id @default(autoincrement())
  placename         String
  description       String
}
model Places{
  id                Int         @id @default(autoincrement())
  city              String
  country           String
  placename         String
  location          String    //need to figure this out 

}